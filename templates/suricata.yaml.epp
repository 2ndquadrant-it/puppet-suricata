%YAML 1.1
---
# Manged by puppet
#
# Suricata configuration file.
#
##
## Step 1: inform Suricata about your network
##

vars:
  # more specifc is better for alert accuracy and performance
  <% $suricata::config_vars.each |$var_key, $var_hash| { %>
  <%= "${var_key}:" %>
  <% $var_hash.each |$key, $value| { -%>
  <%= "${key}: \"${value}\"" %>
  <% } %>
  <% } %>
